    }
    .nav_{
        display: flex;
        white-space: nowrap;
        padding: 5rpx 0;
      }
      .nav__item{
        display: flex;
        padding: 20rpx 40rpx;
        font-size: 30rpx;
        color: gray;
        border-bottom: 1rpx solid gray;
      }
      .will{
        padding: 20rpx 0;
        /* padding-top: 400rpx; */
      }
      .will_{
        display: flex;
        justify-content: space-between;
        font-size: 30rpx;
        padding: 10rpx 40rpx;
      }
      .video_wrap{
        display: flex;
        flex-wrap: wrap;
        padding: 10rpx;
        justify-content: space-between;
      }
      .video_item{
        width: 48%;
        margin-bottom: 10rpx;
      }
      .video_img{
        position: relative;
      }
      .video_img .video_info{
        display: flex;
        position: absolute;
        justify-content: space-between;
        left: 0;
        /* right: 5rpx; */
        bottom: 10rpx;
        width: 100%;
        color: white;
        font-size: 24rpx;
      }
      .video_img image{
        width: 100%;
        border-radius: 16rpx;
      }
      .video_count{
        width: 24rpx;
        padding-right: 24rpx;
      }
      .video_title{
       font-size: 28rpx;
       display: -webkit-box;
       overflow: hidden;
       white-space: normal;
       text-overflow: ellipsis;
       word-wrap: break-word;
       -webkit-line-clamp:2 ;
       -webkit-box-orient: vertical;
      }